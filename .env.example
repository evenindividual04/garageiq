# GarageIQ Environment Variables
# Copy this file to .env and fill in your values

# ===== Required =====
GROQ_API_KEY=your_groq_api_key_here

# ===== Optional =====
# Set to 'true' to use Groq (recommended for production)
AMI_USE_GROQ=true

# Set to 'true' to use local Ollama (for offline development)
AMI_USE_OLLAMA=false
AMI_OLLAMA_MODEL=llama3.2

# Environment mode
AMI_ENV=development

# Translation (set to true if you have NLLB model downloaded)
AMI_USE_NLLB=false
